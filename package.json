{
  "name": "packbin-adapter",
  "version": "0.0.1",
  "description": "Packbin generic adapter",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint src",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "NODE_ENV=test mocha $(find ./test -name \"*.spec.js\")",
    "posttest": "rm $(find ./src ./test -name \"*.js\")",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://git.mstack.net/packbin/adapter.git"
  },
  "keywords": [
    "packbin",
    "adapter",
    "artifact",
    "registry",
    "repository",
    "private",
    "self-hosted",
    "on-premises"
  ],
  "author": "Michael de Wit <mjwwit@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/express": "^4.0.35",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/proxyquire": "^1.3.27",
    "@types/sinon": "^1.16.36",
    "@types/sinon-chai": "^2.7.27",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "proxyquire": "^1.7.11",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "express": "^4.15.2"
  }
}
